  <process id="leadManagementProcess" name="Lead management process" isExecutable="true">
    <serviceTask id="leadCreation" name="Lead Creation" flowable:delegateExpression="${leadCreationServiceTask}"></serviceTask>
    <serviceTask id="buyerRegistration" name="Buyer registration at Owners" flowable:delegateExpression="${buyerRegistrationServiceTask}"></serviceTask>
    <serviceTask id="createSaveSearch" name="Create save search  at Owners" flowable:delegateExpression="${createSaveSearchServiceTask}"></serviceTask>
    <exclusiveGateway id="leadFlowGateway" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="timerStartBuyerRegistration" targetRef="buyerRegistration"></sequenceFlow>
    <sequenceFlow id="flow8" name="Buyer not registered" sourceRef="leadFlowGateway" targetRef="timerStartBuyerRegistration">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isFarmingRequired==true && registered==false && existingContact==true}]]></conditionExpression>
    </sequenceFlow>
    <receiveTask id="waitFarming" name="Wait for farming"></receiveTask>
    <serviceTask id="checkForFarmingEligibility" name="Check for farming eligibility" flowable:delegateExpression="${checkForFarmingEligibilityServiceTask}"></serviceTask>
    <sequenceFlow id="flow12" sourceRef="leadCreation" targetRef="buyerRegistration"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="buyerRegistration" targetRef="createSaveSearch"></sequenceFlow>
    <endEvent id="endLeadManagementEvent" name="End Lead Management Event"></endEvent>
    <sequenceFlow id="flow15" name="Qualified/Lost/Outbound attempt" sourceRef="waitFarming" targetRef="endLeadManagementEvent"></sequenceFlow>
    <sequenceFlow id="flow17" name="New lead from SF" sourceRef="leadFlowGateway" targetRef="leadCreation">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isFarmingRequired==true && existingContact==false && registered==false}]]></conditionExpression>
    </sequenceFlow>
	<intermediateCatchEvent id="timerStartBuyerRegistration" name="TimerCatchEvent">
      <timerEventDefinition>
        <timeDate>${buyerFarmingDelay}</timeDate>
      </timerEventDefinition>
    </intermediateCatchEvent>
	<sequenceFlow id="flow71" sourceRef="checkForFarmingEligibility" targetRef="leadFlowGateway"></sequenceFlow>
    <startEvent id="startevent1" name="Start"></startEvent>
    <sequenceFlow id="flow18" sourceRef="startevent1" targetRef="checkForFarmingEligibility"></sequenceFlow>
    <sequenceFlow id="flow22" name="Non farming lead" sourceRef="leadFlowGateway" targetRef="endLeadManagementEvent">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isFarmingRequired==false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="createSaveSearch" targetRef="waitFarming"></sequenceFlow>
    <sequenceFlow id="flow24" sourceRef="leadFlowGateway" targetRef="waitFarming">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isFarmingRequired==true && existingContact==true && registered==true}]]></conditionExpression>
    </sequenceFlow>
  </process>
